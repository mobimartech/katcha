		{
			"name": "Api Test",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// Configuration\r",
							"const BASE_URL = 'https://social-tracker.automasterpro.net';\r",
							"const API_KEY = 'd4f25ac2a42f35bf7a130dd3743f6e86b2b08f77d13edd116cfcaaadb81ab196';\r",
							"const API_SECRET = '13397ff2fb52da6cc9beeb802bc1e41d35b655268e88007360e149d8744e5784';\r",
							"\r",
							"// Save BASE_URL for use in requests\r",
							"pm.collectionVariables.set(\"BASE_URL\", BASE_URL);\r",
							"\r",
							"// Get current timestamp (epoch seconds)\r",
							"const timestamp = Math.floor(Date.now() / 1000);\r",
							"\r",
							"// Get request method and full path (including query string)\r",
							"const method = pm.request.method;\r",
							"const url = pm.request.url;\r",
							"const path = url.getPath() + (url.getQueryString() ? '?' + url.getQueryString() : '');\r",
							"\r",
							"// Build string to sign\r",
							"const stringToSign = `method=${method}&path=${path}&timestamp=${timestamp}`;\r",
							"\r",
							"// Generate HMAC-SHA256 signature\r",
							"const signature = CryptoJS.HmacSHA256(stringToSign, API_SECRET).toString(CryptoJS.enc.Hex);\r",
							"\r",
							"// Set headers\r",
							"pm.request.headers.upsert({key: 'X-Api-Key', value: API_KEY});\r",
							"pm.request.headers.upsert({key: 'X-Timestamp', value: timestamp.toString()});\r",
							"pm.request.headers.upsert({key: 'X-Signature', value: signature});\r",
							"\r",
							"// Add Authorization header if ACCESS_TOKEN exists\r",
							"const accessToken = pm.collectionVariables.get(\"ACCESS_TOKEN\");\r",
							"if (accessToken) {\r",
							"    pm.request.headers.upsert({key: 'Authorization', value: `Bearer ${accessToken}`});\r",
							"}\r",
							"\r",
							"console.log('Method:', method);\r",
							"console.log('Path:', path);\r",
							"console.log('Timestamp:', timestamp);\r",
							"console.log('String to sign:', stringToSign);\r",
							"console.log('Signature:', signature);\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
